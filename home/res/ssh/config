include mobnet
include stp
include virtual_machines

Host *
    ServerAliveInterval 60
    SetEnv TERM=xterm

Host wert.dk
    HostName wert.dk
    Port 23432
    User ask
    PreferredAuthentications publickey
    IdentityFile ~/.ssh/personal

Host kruhlmann.dev
    HostName 188.177.19.193
    User ges
    Port 22
    PreferredAuthentications publickey
    IdentityFile ~/.ssh/personal
    LocalForward 7031 192.168.1.1:80 # Router

Host nymann nymann.dev
    HostName nymann.dev
    User ges
    Port 23432
    PreferredAuthentications publickey
    IdentityFile ~/.ssh/personal

# Version control
Host github.com github
    User git
    Hostname github.com
    Port 22
    PreferredAuthentications publickey
    IdentityFile ~/.ssh/personal

Host gitlab.com gitlab
    User git
    Hostname gitlab.com
    Port 22
    PreferredAuthentications publickey
    IdentityFile ~/.ssh/personal

Host aur.archlinux.org aur
  User aur
  Hostname aur.archlinux.org
  PreferredAuthentications publickey
  IdentityFile ~/.ssh/personal
